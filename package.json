{
  "name": "ts-simple-automapper-nestjs",
  "version": "1.0.0",
  "description": "Injectable Mapper service for using ts-simple-automapper in NestJS.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
