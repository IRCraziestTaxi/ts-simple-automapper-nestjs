    "tsc": "tsc -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IRCraziestTaxi/ts-simple-automapper-nestjs.git"
  },
  "keywords": [
    "typescript",
    "automapper",
    "ts-simple-automapper",
    "nestjs"
  ],
  "author": "IRCraziestTaxi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IRCraziestTaxi/ts-simple-automapper-nestjs/issues"
  },
  "homepage": "https://github.com/IRCraziestTaxi/ts-simple-automapper-nestjs#readme",
  "devDependencies": {
    "@nestjs/common": "^10.0.4",
    "@nestjs/testing": "^10.0.4",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.1",
    "ts-simple-automapper": "^1.0.3",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "ts-simple-automapper": "^1.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
